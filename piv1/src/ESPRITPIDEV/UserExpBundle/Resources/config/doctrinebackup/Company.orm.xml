<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ESPRITPIDEV\UserExpBundle\Entity\Company" table="company">
    <indexes>
      <index name="idrepresentant" columns="idrepresentant"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="254" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="integer" column="telephone" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="254" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="logo" type="string" column="logo" length="254" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address1" type="string" column="address1" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address2" type="string" column="address2" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codepostal" type="integer" column="codePostal" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idrepresentant" type="integer" column="idrepresentant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
