<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ESPRITPIDEV\UserExpBundle\Entity\Cities" table="cities">
    <id name="cityId" type="integer" column="city_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="countrySlug" type="string" column="country_slug" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="regionSlug" type="string" column="region_slug" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
